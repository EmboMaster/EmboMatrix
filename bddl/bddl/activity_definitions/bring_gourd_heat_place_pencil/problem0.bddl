    (:objects
        gourd.n.02_1 - gourd.n.02
        pencil_box.n.01_1 - pencil_box.n.01
        microwave.n.02_1 - microwave.n.02
        shelf.n.01_1 - shelf.n.01
        desk.n.01_1 - desk.n.01
        piano.n.01_1 - piano.n.01
