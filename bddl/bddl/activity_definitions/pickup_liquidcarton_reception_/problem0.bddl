        (ontop agent.n.01_1 floor.n.01_1)
        (inroom reception_desk.n.01_1 break_room_0)
        (inroom liquid_carton.n.01_1 break_room_0)
        (inroom floor.n.01_1 break_room_0)
        (inroom desk.n.01_1 shared_office_0)
