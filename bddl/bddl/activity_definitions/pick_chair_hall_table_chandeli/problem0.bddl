(define (problem pick_up_the_straight_chair_from_the_hall_arch_wood_empty_room_0_and_put_it_on_top_of_the_conference_table_in_the_hall_arch_wood_empty_room_0_toggle_on_the_chandelier_in_the_hall_arch_wood_empty_room_0)
    (:domain omnigibson)
    (:objects
        straight_chair.n.01_1 - straight_chair.n.01
        conference_table.n.01_1 - conference_table.n.01
        chandelier.n.01_1 - chandelier.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (inroom straight_chair.n.01_1 empty_room_0)
        (inroom conference_table.n.01_1 empty_room_0)
        (inroom chandelier.n.01_1 empty_room_0)
        (inroom floor.n.01_1 empty_room_0)
        (ontop agent.n.01_1 floor.n.01_1)
        (not
            (toggled_on chandelier.n.01_1)
        )
    )

    (:goal
        (and
            (ontop ?straight_chair.n.01_1 ?conference_table.n.01_1)
            (toggled_on ?chandelier.n.01_1)
        )
    )
)