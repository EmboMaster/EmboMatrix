(define (problem please_take_the_letter_from_the_floor_in_empty_room_0_and_place_it_on_the_shelf_in_empty_room_0_and_also_bring_the_soap_dispenser_from_the_floor_in_empty_room_0_and_place_it_next_to_the_papers_on_the_shelf_in_empty_room_0)
    (:domain omnigibson)
    (:objects
        letter.n.01_1 - letter.n.01
        soap_dispenser.n.01_1 - soap_dispenser.n.01
        shelf.n.01_1 - shelf.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop letter.n.01_1 floor.n.01_1)
        (ontop soap_dispenser.n.01_1 floor.n.01_1)
        (inroom shelf.n.01_1 empty_room_0)
        (inroom floor.n.01_1 empty_room_0)
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
        (and
            (ontop ?letter.n.01_1 ?shelf.n.01_1)
            (nextto ?soap_dispenser.n.01_1 ?letter.n.01_1)
        )
    )
)