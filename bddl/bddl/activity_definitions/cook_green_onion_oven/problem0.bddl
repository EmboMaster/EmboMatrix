(define (problem go_to_the_kitchen_0_pick_up_the_green_onion_n_01_from_the_reception_desk_open_the_oven_n_01_put_the_green_onion_n_01_inside_the_oven_n_01_close_the_oven_n_01_and_cook_the_green_onion_n_01_in_the_oven_n_01)
    (:domain omnigibson)
    (:objects
        green_onion.n.01_1 - green_onion.n.01
        reception_desk.n.01_1 - reception_desk.n.01
        oven.n.01_1 - oven.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop green_onion.n.01_1 reception_desk.n.01_1)
        (not (open oven.n.01_1))
        (not (toggled_on oven.n.01_1))
        (inroom reception_desk.n.01_1 kitchen_0)
        (inroom oven.n.01_1 kitchen_0)
        (inroom floor.n.01_1 kitchen_0)
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
        (and
            (cooked ?green_onion.n.01_1)
            (inside ?green_onion.n.01_1 ?oven.n.01_1)
            (not (open ?oven.n.01_1))
            (toggled_on ?oven.n.01_1)
        )
    )
)