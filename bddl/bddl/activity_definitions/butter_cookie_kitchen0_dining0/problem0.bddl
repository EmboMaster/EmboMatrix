(define (problem pick_up_the_butter_cookie_from_the_reception_desk_in_kitchen_0_and_cook_it_using_the_oven_in_kitchen_0_then_bring_the_cooked_butter_cookie_to_the_breakfast_table_in_dining_room_0_and_place_it_on_top_of_the_plate)
    (:domain omnigibson)
    (:objects
        butter_cookie.n.01_1 - butter_cookie.n.01
        reception_desk.n.01_1 - reception_desk.n.01
        oven.n.01_1 - oven.n.01
        breakfast_table.n.01_1 - breakfast_table.n.01
        plate.n.04_1 - plate.n.04
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init 
        (ontop butter_cookie.n.01_1 reception_desk.n.01_1)
        (inroom reception_desk.n.01_1 kitchen_0)
        (inroom oven.n.01_1 kitchen_0)
        (inroom floor.n.01_1 kitchen_0)
        (ontop agent.n.01_1 floor.n.01_1)
        (inroom breakfast_table.n.01_1 dining_room_0)
        (ontop plate.n.04_1 breakfast_table.n.01_1)
        (inroom floor.n.01_1 dining_room_0)
        (not 
            (toggled_on oven.n.01_1)
        )
    )

    (:goal 
        (and 
            (cooked ?butter_cookie.n.01_1)
            (ontop ?butter_cookie.n.01_1 ?plate.n.04_1)
        )
    )
)