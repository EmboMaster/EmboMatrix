(define (problem go_to_closet_1_pick_up_the_grapefruit_from_the_shelf_in_closet_1_and_freeze_it_by_putting_it_inside_the_cooler_in_corridor_0_then_go_to_bedroom_0_pick_up_the_bread_slice_from_the_desk_in_bedroom_0_and_heat_it_using_the_toaster_oven_in_bathroom_0)
    (:domain omnigibson)
    (:objects
        grapefruit.n.02_1 - grapefruit.n.02
        shelf.n.01_1 - shelf.n.01
        cooler.n.01_1 - cooler.n.01
        bread_slice.n.01_1 - bread_slice.n.01
        desk.n.01_1 - desk.n.01
        toaster_oven.n.01_1 - toaster_oven.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop grapefruit.n.02_1 shelf.n.01_1)
        (inroom shelf.n.01_1 closet_1)
        (inroom cooler.n.01_1 corridor_0)
        (ontop bread_slice.n.01_1 desk.n.01_1)
        (inroom desk.n.01_1 bedroom_0)
        (not (toggled_on toaster_oven.n.01_1))
        (inroom toaster_oven.n.01_1 bathroom_0)
        (ontop agent.n.01_1 floor.n.01_1)
        (inroom floor.n.01_1 closet_1)
    )

    (:goal
        (and
            (frozen ?grapefruit.n.02_1)
            (inside ?grapefruit.n.02_1 ?cooler.n.01_1)
            (hot ?bread_slice.n.01_1)
            (inside ?bread_slice.n.01_1 ?toaster_oven.n.01_1)
        )
    )
)