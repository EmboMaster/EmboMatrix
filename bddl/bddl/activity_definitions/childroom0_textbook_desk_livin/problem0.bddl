(define (problem go_to_the_childs_room_0_pick_up_the_textbook_from_the_floor_and_put_it_on_top_of_the_desk_in_the_childs_room_0_then_go_to_the_living_room_0_toggle_on_the_table_lamp_n_01_and_pick_up_the_security_camera_from_the_sofa_to_place_it_on_the_coffee_table_in_the_living_room_0)
    (:domain omnigibson)
    (:objects
        textbook.n.01_1 - textbook.n.01
        desk.n.01_1 - desk.n.01
        table_lamp.n.01_1 - table_lamp.n.01
        security_camera.n.01_1 - security_camera.n.01
        sofa.n.01_1 - sofa.n.01
        coffee_table.n.01_1 - coffee_table.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop textbook.n.01_1 floor.n.01_1)
        (ontop floor.n.01_1 floor.n.01_1) ; Agent is on the floor
        (inroom textbook.n.01_1 childs_room_0)
        (inroom desk.n.01_1 childs_room_0)
        (inroom table_lamp.n.01_1 living_room_0)
        (inroom security_camera.n.01_1 living_room_0)
        (inroom sofa.n.01_1 living_room_0)
        (inroom coffee_table.n.01_1 living_room_0)
        (ontop security_camera.n.01_1 sofa.n.01_1)
        (not (toggled_on table_lamp.n.01_1))
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
        (and
            (ontop ?textbook.n.01_1 ?desk.n.01_1)
            (toggled_on ?table_lamp.n.01_1)
            (ontop ?security_camera.n.01_1 ?coffee_table.n.01_1)
        )
    )
)