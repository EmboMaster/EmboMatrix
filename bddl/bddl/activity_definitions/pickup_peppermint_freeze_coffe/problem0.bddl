(define (problem pick_up_the_peppermint.n.01_from_the_table_in_shared_office_0_and_put_it_inside_the_cooler.n.01_in_the_lobby_0_to_freeze_it_then_toggle_on_the_coffee_maker.n.01_in_the_lobby_0_to_prepare_coffee-0)
