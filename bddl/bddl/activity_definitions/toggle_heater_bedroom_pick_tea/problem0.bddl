        space_heater.n.01_1 - space_heater.n.01
        teacup.n.02_1 - teacup.n.02
        wardrobe.n.01_1 - wardrobe.n.01
        electric_kettle.n.01_1 - electric_kettle.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

