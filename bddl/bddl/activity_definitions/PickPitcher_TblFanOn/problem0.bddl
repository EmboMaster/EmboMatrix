(define (problem Please_pick_up_the_pitcher_from_the_reception_desk_in_the_kitchen_0_and_place_it_on_the_breakfast_table_in_the_dining_room_0_Then_toggle_on_the_electric_fan_n_01_near_the_breakfast_table_in_the_dining_room_0_to_cool_the_area_for_customers)
    (:domain omnigibson)
    (:objects
        pitcher.n.02_1 - pitcher.n.02
        reception_desk.n.01_1 - reception_desk.n.01
        breakfast_table.n.01_1 - breakfast_table.n.01
        electric_fan.n.01_1 - electric_fan.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )
    
    (:init
        (ontop pitcher.n.02_1 reception_desk.n.01_1)
        (inroom reception_desk.n.01_1 kitchen_0)
        (inroom breakfast_table.n.01_1 dining_room_0)
        (ontop electric_fan.n.01_1 breakfast_table.n.01_1)
        (not (toggled_on electric_fan.n.01_1))
        (inroom electric_fan.n.01_1 dining_room_0)
        (inroom floor.n.01_1 kitchen_0)
        (ontop agent.n.01_1 floor.n.01_1)
    )
    
    (:goal
        (and
            (ontop ?pitcher.n.02_1 ?breakfast_table.n.01_1)
            (toggled_on ?electric_fan.n.01_1)
        )
    )
)