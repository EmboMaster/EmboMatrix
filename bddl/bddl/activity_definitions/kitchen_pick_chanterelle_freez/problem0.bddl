(define (problem go_to_the_kitchen_0_pick_up_the_chanterelle.n.01_from_the_booth_and_bring_it_to_the_cooler.n.01_put_the_chanterelle.n.01_inside_the_cooler.n.01_to_freeze_it_afterward_move_to_the_living_room_0_and_toggle_on_the_room_light.n.01_for_better_visibility)
    (:domain omnigibson)
    (:objects
        chanterelle.n.01_1 - chanterelle.n.01
        booth.n.01_1 - booth.n.01
        cooler.n.01_1 - cooler.n.01
        room_light.n.01_1 - room_light.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop chanterelle.n.01_1 booth.n.01_1)
        (inroom booth.n.01_1 kitchen_0)
        (inroom cooler.n.01_1 kitchen_0)
        (not (toggled_on room_light.n.01_1))
        (inroom room_light.n.01_1 living_room_0)
        (inroom floor.n.01_1 kitchen_0)
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
        (and
            (frozen ?chanterelle.n.01_1)
            (inside ?chanterelle.n.01_1 ?cooler.n.01_1)
            (toggled_on ?room_light.n.01_1)
        )
    )
)