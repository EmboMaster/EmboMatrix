            (ontop ?lunch_box.n.01_1 ?booth.n.01_1)
            (open ?microwave.n.02_1)
            (hot ?chocolate_cake.n.01_1)
            (inside ?chocolate_cake.n.01_1 ?microwave.n.02_1)
        )
    )
)