(define (problem please_pick_up_the_carton_from_the_shelf_in_the_copy_room_0_go_to_the_meeting_room_0_open_the_microwave_n_02_put_the_tortilla_n_01_inside_the_microwave_n_02_toggle_on_the_microwave_n_02_and_heat_the_tortilla_n_01)
    (:domain omnigibson)
    (:objects
        carton.n.02_1 - carton.n.02
        shelf.n.01_1 - shelf.n.01
        microwave.n.02_1 - microwave.n.02
        tortilla.n.01_1 - tortilla.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (inside carton.n.02_1 shelf.n.01_1)
        (inside tortilla.n.01_1 carton.n.02_1)
        (inroom shelf.n.01_1 copy_room_0)
        (inroom microwave.n.02_1 meeting_room_0)
        (not (toggled_on microwave.n.02_1))
        (ontop agent.n.01_1 floor.n.01_1)
        (inroom floor.n.01_1 copy_room_0)
    )

    (:goal
        (and
            (inside ?tortilla.n.01_1 ?microwave.n.02_1)
            (cooked ?tortilla.n.01_1)
            (toggled_on ?microwave.n.02_1)
        )
    )
)