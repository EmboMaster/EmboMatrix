    (:objects
        arugula.n.02_1 - arugula.n.02
        countertop.n.01_1 - countertop.n.01
        mop_bucket.n.01_1 - mop_bucket.n.01
        space_heater.n.01_1 - space_heater.n.01
        floor.n.01_1 - floor.n.01
