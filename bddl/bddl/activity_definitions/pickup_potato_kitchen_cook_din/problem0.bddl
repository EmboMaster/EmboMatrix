(define (problem pick_up_the_potato_from_the_reception_desk_in_the_kitchen_0_open_the_oven_in_the_kitchen_0_cook_the_potato_using_the_oven_and_place_the_cooked_potato_on_the_breakfast_table_in_the_dining_room_0)
    (:domain omnigibson)
    (:objects
        potato.n.01_1 - potato.n.01
        reception_desk.n.01_1 - reception_desk.n.01
        oven.n.01_1 - oven.n.01
        breakfast_table.n.01_1 - breakfast_table.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop potato.n.01_1 reception_desk.n.01_1)
        (ontop reception_desk.n.01_1 floor.n.01_1)
        (not (toggled_on oven.n.01_1))
        (not (open oven.n.01_1))
        (inroom reception_desk.n.01_1 kitchen_0)
        (inroom oven.n.01_1 kitchen_0)
        (inroom floor.n.01_1 kitchen_0)
        (inroom breakfast_table.n.01_1 dining_room_0)
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
        (and
            (cooked ?potato.n.01_1)
            (ontop ?potato.n.01_1 ?breakfast_table.n.01_1)
        )
    )
)