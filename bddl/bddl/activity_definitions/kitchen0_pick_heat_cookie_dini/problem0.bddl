(define (problem go_to_room_kitchen_0_pick_up_the_sugar_cookie_from_the_reception_desk_in_kitchen_0_heat_the_sugar_cookie_using_the_microwave_in_kitchen_0_go_to_room_dining_room_0_put_the_sugar_cookie_on_the_platter_on_the_board_in_dining_room_0)
    (:domain omnigibson)
    (:objects
        sugar_cookie.n.01_1 - sugar_cookie.n.01
        reception_desk.n.01_1 - reception_desk.n.01
        microwave.n.02_1 - microwave.n.02
        platter.n.01_1 - platter.n.01
        board.n.02_1 - board.n.02
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop sugar_cookie.n.01_1 reception_desk.n.01_1)
        (ontop reception_desk.n.01_1 floor.n.01_1)
        (not (toggled_on microwave.n.02_1))
        (ontop platter.n.01_1 board.n.02_1)
        (inroom reception_desk.n.01_1 kitchen_0)
        (inroom microwave.n.02_1 kitchen_0)
        (inroom floor.n.01_1 kitchen_0)
        (inroom board.n.02_1 dining_room_0)
        (inroom platter.n.01_1 dining_room_0)
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
        (and
            (ontop ?sugar_cookie.n.01_1 ?platter.n.01_1)
            (cooked ?sugar_cookie.n.01_1)
        )
    )
)