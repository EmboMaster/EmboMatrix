        rice_cooker.n.01_1 - rice_cooker.n.01
        tortilla.n.01_1 - tortilla.n.01
        desk.n.01_1 - desk.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (inroom rice_cooker.n.01_1 corridor_0)
        (not (toggled_on rice_cooker.n.01_1))
        (inroom tortilla.n.01_1 corridor_0)
        (inroom desk.n.01_1 bedroom_0)
        (inroom floor.n.01_1 corridor_0)
