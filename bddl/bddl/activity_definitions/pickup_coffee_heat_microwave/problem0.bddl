(define (problem pick_up_the_coffee_cup_from_the_vending_machine_area_in_shared_office_0_go_to_break_room_0_and_place_the_coffee_cup_on_the_reception_desk_near_the_microwave_toggle_on_the_microwave_in_break_room_0_heat_the_coffee_cup_inside_the_microwave_and_toggle_off_the_microwave_afterward)
    (:domain omnigibson)
    (:objects
        coffee_cup.n.01_1 - coffee_cup.n.01
        vending_machine.n.01_1 - vending_machine.n.01
        microwave.n.02_1 - microwave.n.02
        reception_desk.n.01_1 - reception_desk.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop coffee_cup.n.01_1 vending_machine.n.01_1)
        (inroom vending_machine.n.01_1 shared_office_0)
        (inroom microwave.n.02_1 break_room_0)
        (inroom reception_desk.n.01_1 break_room_0)
        (not (toggled_on microwave.n.02_1))
        (ontop agent.n.01_1 floor.n.01_1)
        (inroom floor.n.01_1 shared_office_0)
    )

    (:goal
        (and
            (inside ?coffee_cup.n.01_1 ?microwave.n.02_1)
            (hot ?coffee_cup.n.01_1)
            (not (toggled_on ?microwave.n.02_1))
        )
    )
)