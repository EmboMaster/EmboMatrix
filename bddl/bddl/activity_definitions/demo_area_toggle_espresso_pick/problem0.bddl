    (:objects
        espresso_machine.n.01_1 - espresso_machine.n.01
        vidalia_onion.n.01_1 - vidalia_onion.n.01
        electric_kettle.n.01_1 - electric_kettle.n.01
        shelf_baseboard.n.01_1 - shelf_baseboard.n.01
