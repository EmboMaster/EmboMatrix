(define (problem go_to_the_demonstration_area_in_grocery_store_0_and_toggle_on_the_espresso_machine.n.01_then_pick_up_the_vidalia_onion.n.01_from_the_shelf_baseboard_in_grocery_store_0_and_put_it_inside_the_electric_kettle.n.01_also_located_in_grocery_store_0-0)
