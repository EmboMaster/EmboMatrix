(define (problem please_pick_up_the_mango_from_the_reception_desk_in_the_kitchen_0_cook_it_using_the_rice_cooker_n_01_in_the_kitchen_0_and_place_the_cooked_mango_on_the_breakfast_table_in_the_dining_room_0)
    (:domain omnigibson)
    (:objects
        mango.n.02_1 - mango.n.02
        reception_desk.n.01_1 - reception_desk.n.01
        rice_cooker.n.01_1 - rice_cooker.n.01
        breakfast_table.n.01_1 - breakfast_table.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop mango.n.02_1 reception_desk.n.01_1)
        (not (toggled_on rice_cooker.n.01_1))
        (inroom reception_desk.n.01_1 kitchen_0)
        (inroom rice_cooker.n.01_1 kitchen_0)
        (inroom floor.n.01_1 kitchen_0)
        (inroom breakfast_table.n.01_1 dining_room_0)
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
