        chocolate_chip_cookie.n.01_1 - chocolate_chip_cookie.n.01
        platter.n.01_1 - platter.n.01
        microwave.n.02_1 - microwave.n.02
        table_lamp.n.01_1 - table_lamp.n.01
        floor.n.01_1 - floor.n.01
