(define (problem pick_up_the_lasagna_from_the_reception_desk_in_the_kitchen_0_and_cook_it_using_the_oven_in_the_kitchen_0_then_place_the_cooked_lasagna_on_the_breakfast_table_in_the_dining_room_0_for_serving)
    (:domain omnigibson)
    (:objects
        lasagna.n.01_1 - lasagna.n.01
        reception_desk.n.01_1 - reception_desk.n.01
        oven.n.01_1 - oven.n.01
        breakfast_table.n.01_1 - breakfast_table.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop lasagna.n.01_1 reception_desk.n.01_1)
        (not (toggled_on oven.n.01_1))
        (inroom reception_desk.n.01_1 kitchen_0)
        (inroom oven.n.01_1 kitchen_0)
        (inroom floor.n.01_1 kitchen_0)
        (inroom breakfast_table.n.01_1 dining_room_0)
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
        (and
            (cooked ?lasagna.n.01_1)
            (ontop ?lasagna.n.01_1 ?breakfast_table.n.01_1)
        )
    )
)