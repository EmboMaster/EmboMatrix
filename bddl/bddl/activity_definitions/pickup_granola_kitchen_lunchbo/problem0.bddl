    (:objects
        granola_bar.n.01_1 - granola_bar.n.01
        table_lamp.n.01_1 - table_lamp.n.01
        microwave.n.02_1 - microwave.n.02
        lunch_box.n.01_1 - lunch_box.n.01
        reception_desk.n.01_1 - reception_desk.n.01
