(define (problem pick_up_the_orange_from_the_reception_desk_in_bathroom_0_go_to_hall_arch_wood_empty_room_0_and_put_the_orange_inside_the_electric_cauldron_in_hall_arch_wood_empty_room_0_then_cook_the_orange_inside_the_electric_cauldron_in_hall_arch_wood_empty_room_0)
    (:domain omnigibson)
    (:objects
        orange.n.01_1 - orange.n.01
        reception_desk.n.01_1 - reception_desk.n.01
        electric_cauldron.n.01_1 - electric_cauldron.n.01
        floor.n.01_1 - floor.n.01
        agent.n.01_1 - agent.n.01
    )

    (:init
        (ontop orange.n.01_1 reception_desk.n.01_1)
        (inroom reception_desk.n.01_1 bathroom_0)
        (inroom electric_cauldron.n.01_1 empty_room_0)
        (ontop agent.n.01_1 floor.n.01_1)
        (inroom floor.n.01_1 bathroom_0)
        (not
            (toggled_on electric_cauldron.n.01_1)
        )
    )

    (:goal
        (and
            (inside ?orange.n.01_1 ?electric_cauldron.n.01_1)
            (cooked ?orange.n.01_1)
            (toggled_on ?electric_cauldron.n.01_1)
        )
    )
)