    (:objects
        lemon.n.01_1 - lemon.n.01
        wicker_basket.n.01_1 - wicker_basket.n.01
        table_knife.n.01_1 - table_knife.n.01
        water_bottle.n.01_1 - water_bottle.n.01
        console_table.n.01_1 - console_table.n.01
